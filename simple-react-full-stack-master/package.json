{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "clear": "rimraf -rf dist",
    "lint": "eslint server.js \"src/**/*.js\"",
    "prebuild": "npm run clear",
    "build:server": "babel -d dist/server ./src/server/",
    "build:client": "webpack",
    "build": "npm run build:client && npm run build:server",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open nodemon --ignore dist/ --ignore src/ --exec \"npm run build:server && node dist/server/index.js\"",
    "start": "node dist/server/index.js"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "ag-grid-community": "^20.0.0",
    "ag-grid-enterprise": "^20.0.0",
    "ag-grid-react": "^20.0.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.23.0",
    "body-parser": "^1.18.3",
    "bootstrap-sass": "^3.4.0",
    "connect-history-api-fallback": "^1.6.0",
    "cross-env": "^5.1.1",
    "express": "^4.16.3",
    "lodash.times": "^4.3.2",
    "materialize-css": "^1.0.0",
    "materialize-sass": "^0.98.2",
    "otplib": "^10.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-datepicker": "^2.1.0",
    "react-dom": "^16.5.2",
    "react-hot-loader": "next",
    "react-json-editor-ajrm": "^2.5.9",
    "react-json-pretty": "^2.0.0",
    "react-materialize": "^2.6.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-form": "^8.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.85.0",
    "webpack-dev-middleware": "^3.5.2",
    "webpack-hot-middleware": "^2.24.3"
  },
