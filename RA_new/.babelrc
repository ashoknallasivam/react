{
  "presets": [
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "modules": false
    }],
    "es2015",
	"react",
	"stage-1"
  ],
  "plugins": [
    "transform-object-rest-spread",
	"@babel/plugin-transform-regenerator"
  ],
  "env": {
    "development": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}


