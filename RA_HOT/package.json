{
  "name": "RapterAdmin",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build:client": "webpack --mode production",
    "build:server": "babel -d dist/server ./src/server/",
    "build": "npm run build:client && npm run build:server",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "dev": "concurrently \"npm run build:server && nodemon dist/server/index.js\" \"npm run client\""
  },
  "author": "SM NETSERV",
  "license": "ISC",
